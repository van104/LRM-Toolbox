{
  "id": "regex",
  "name": "RegEx 正则表达式",
  "sections": [
    {
      "title": "元字符",
      "items": [
        {
          "cmd": ".",
          "desc": "匹配除换行符外任意字符"
        },
        {
          "cmd": "\\d, \\w, \\s",
          "desc": "数字, 字母, 空白符"
        },
        {
          "cmd": "^, $",
          "desc": "匹配开头, 结尾"
        },
        {
          "cmd": "[a-z]",
          "desc": "匹配字符集范围"
        }
      ]
    },
    {
      "title": "量词",
      "items": [
        {
          "cmd": "*",
          "desc": "匹配 0 次或多次"
        },
        {
          "cmd": "+",
          "desc": "匹配 1 次或多次"
        },
        {
          "cmd": "?",
          "desc": "匹配 0 次 or 1 次"
        },
        {
          "cmd": "{n,m}",
          "desc": "匹配 n 到 m 次"
        }
      ]
    }
  ]
}